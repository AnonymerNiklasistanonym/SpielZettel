{
  "$schema": "https://raw.githubusercontent.com/SchemaStore/schemastore/refs/heads/master/src/schemas/json/web-manifest.json",
  "id": "github.com/AnonymerNiklasistanonym/SpielZettel",
  "name": "SpielZettel",
  "short_name": "SpielZettel",
  "icons": [
    {
      "src": "CUSTOM_ASSET_PREFIX/icons/favicon-192x192.png",
      "type": "image/png",
      "sizes": "192x192"
    },
    {
      "src": "CUSTOM_ASSET_PREFIX/icons/favicon-512x512.png",
      "type": "image/png",
      "sizes": "512x512"
    },
    {
      "src": "CUSTOM_ASSET_PREFIX/favicon.svg",
      "sizes": "any",
      "type": "image/svg+xml"
    }
  ],
  "theme_color": "#f1f1f1",
  "background_color": "#000000",
  "start_url": "CUSTOM_ASSET_PREFIX/",
  "display": "standalone",
  "orientation": "any",
  "file_handlers": [
    {
      "action": "CUSTOM_ASSET_PREFIX/",
      "accept": {
        "application/x-spielzettel": [".spielzettel"]
      }
    }
  ],
  "screenshots": [
    {
      "src": "CUSTOM_ASSET_PREFIX/icons/favicon-1024x512.png",
      "sizes": "1024x512",
      "type": "image/png",
      "label": "Wide Screenshot",
      "form_factor": "wide"
    },
    {
      "src": "CUSTOM_ASSET_PREFIX/icons/favicon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "label": "Mobile Screenshot"
    }
  ]
}
